{
  "expo": {
    "name": "OurRecipes",
    "slug": "OurRecipes",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "uk.co.meadbros.ourrecipes",
      "versionCode": 2
    },
    "description": "Exploring React Native App, eventually intended to be a simple recipe app",
    "githubUrl": "https://github.com/JM3ad/ReactNativeRecipes"
  }
}
